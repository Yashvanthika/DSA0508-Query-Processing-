import pandas as pd

# Create DataFrame
df = pd.DataFrame({
    'School': ['S1', 'S1', 'S2', 'S2'],
    'Class': ['A', 'B', 'A', 'B'],
    'Age': [10, 12, 11, 13]
})

print("Original DataFrame:")
print(df)

# Group by School and Class
grouped = df.groupby(['School', 'Class'])

print("\nGrouped DataFrame:")
for name, group in grouped:
    print("\nGroup:", name)
    print(group)
