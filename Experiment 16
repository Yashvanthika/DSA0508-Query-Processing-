import pandas as pd

# Create DataFrame
df = pd.DataFrame({
    'School': ['S1', 'S1', 'S2', 'S2'],
    'Age': [10, 12, 11, 13]
})

print("Original DataFrame:")
print(df)

# Group by School
grouped = df.groupby('School')

print("\nGrouped DataFrame:")
for name, group in grouped:
    print("\nSchool Code:", name)
    print(group)

print("\nType of GroupBy object:")
print(type(grouped))
